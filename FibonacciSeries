 class FibonacciSeriesUsingThreads1 extends Thread
{
	FibonacciSeriesUsingThreads1()
	{
		//setName("Fibonacii Series");
	}
	public void run()
	{
		int a=0;
		int b=1;
		int c;
		System.out.println(  a+"\n"+ b);
		for (int i=0;i<=7;i++)
			{
			c=a+b;
			a=b;
			b=c;
			System.out.println(c);
			}
	}
}
		/*public void start()
		{
			
		}*/
		public class FibonacciSeriesUsingThreads
		{
	public static void main(String[] args)
		{
		System.out.println("Fibonacci seriesss");
		FibonacciSeriesUsingThreads1 fb = new FibonacciSeriesUsingThreads1();
		fb.setName("Fibonacci");
		fb.run();
		}
		}
	

